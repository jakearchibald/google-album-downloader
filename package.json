    "postcss-url": "^8.0.0",
    "preact": "^10.2.1",
    "preact-render-to-string": "^5.1.3",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "typescript": "^3.7.4"
